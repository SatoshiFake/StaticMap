{
    "name": "wyrihaximus/staticmap",
    "description": "PHP implementation of the Google Maps Static API",
    "homepage": "http://wyrihaximus.net/projects/php/staticmap.html",
    "license": "MIT",
    "keywords": [
        "maps",
        "static maps",
        "image processing",
        "graphics"
    ],
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com",
            "homepage": "http://wyrihaximus.net/"
        }
    ],
    "require": {
        "php": ">=5.4",
        "imagine/Imagine": "0.4.*",
        "react/promise": "~2.0",
        "react/stream": "0.4.*",
        "react/event-loop": "0.4.*",
        "wyrihaximus/react-guzzle": "dev-master"
    },
    "require-dev": {
        "satooshi/php-coveralls": "~0.6",
        "ext-gd": "*",
        "phpunit/phpunit": "~4.0",
        "vectorface/dunit": "~1.0",
        "squizlabs/php_codesniffer": "~1.0",
        "object-calisthenics/phpcs-calisthenics-rules": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\StaticMap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\StaticMap\\Tests\\": "tests/"
        }
    }
